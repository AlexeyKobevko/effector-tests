{
  "name": "anonymous",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch server.ts --watch src/entry-server.tsx --exec 'yarn ts-node server.ts'",
    "build": "tsc -p tsconfig.prod.json && yarn build:client && yarn build:server && yarn copy-files",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "serve": "yarn build && cross-env NODE_ENV=production node --experimental-modules dist/server.js",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles index.html dist",
    "lint": "eslint --debug \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --debug \"src/**/*.{ts,tsx}\" --fix",
    "lint:fix:nodebug": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc -p tsconfig.json",
    "test": "lint-staged",
    "test-watch": "jest --watch",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .ts --ext .tsx"
    ]
  },
  "browserslist": [
    "last 2 version",
    "safari >= 10"
  ],
  "dependencies": {
    "@hookform/resolvers": "2.8.8",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "compression": "1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.26.0",
    "effector": "^21.8.12",
    "effector-react": "^21.3.3",
    "effector-storage": "^4.3.0",
    "express": "4.17.1",
    "nodemon": "^2.0.12",
    "react": "^17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.0",
    "react-hook-form": "7.27.0",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-transition-group": "^4.4.2",
    "serve-static": "^1.14.1",
    "shipit-cli": "^5.3.0",
    "shipit-deploy": "^5.3.0",
    "swiper": "^7.3.2",
    "ts-node": "^10.1.0",
    "universal-cookie": "^4.0.4",
    "universal-cookie-express": "^4.0.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@hookform/devtools": "4.0.2",
    "@tailwindcss/line-clamp": "^0.2.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/compression": "1.7.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/md5": "^2.3.1",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-transition-group": "^4.4.2",
    "@types/swiper": "^5.4.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@vitejs/plugin-legacy": "^1.6.4",
    "@vitejs/plugin-react": "^1.2.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.4.2",
    "copyfiles": "^2.4.1",
    "dotenv": "^9.0.0",
    "effector-logger": "^0.10.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.5",
    "lint-staged": "^11.1.2",
    "msw": "^0.36.7",
    "postcss": "^8.4.5",
    "prettier": "^2.3.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "tailwind-scrollbar": "^1.3.1",
    "tailwind-scrollbar-hide": "^1.1.6",
    "tailwindcss": "^3.0.7",
    "ts-jest": "^27.1.1",
    "typescript": "4.5.3",
    "vite": "2.8.0",
    "vite-plugin-legacy": "^2.1.0",
